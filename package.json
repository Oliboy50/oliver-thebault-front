{
  "name": "oliver-thebault",
  "version": "0.1.0",
  "description": "Oliver Thébault's personal website",
  "author": "Oliver Thébault",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run build:dist",
    "build:dist": "babel-node tools/distServer.js",
    "build:html": "babel-node tools/buildHtml.js",
    "build:src": "babel-node tools/srcServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist",
    "prestart": "npm run remove-dist",
    "start": "npm-run-all --parallel test:watch build:src lint:watch",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "font-awesome": "4.6.3",
    "material-ui": "0.15.0",
    "normalize.css": "4.1.1",
    "react": "15.1.*",
    "react-dom": "15.1.*",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "copy-webpack-plugin": "3.0.1",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "npm-run-all": "1.8.0",
    "postcss-loader": "0.9.1",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
